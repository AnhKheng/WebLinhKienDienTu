<script>
    function checkSummary(productSummary){
        var summary = [];
        if(productSummary){
            var splitSummary = productSummary.split("\n");

            splitSummary.forEach(function(value,item){
                if(item < 5 && 1 < 2) {
                    summary.push(`<div class="item">${splitSummary[item]}</div>`);
                }
            })
            if (splitSummary.length > 5) {
                summary.push(`<div class="item">...</div>`);
            }
        }
        return summary.join('');
    }

    function checkKhuyenMai(specialOffer){
        var offer = [];

        if(specialOffer){
            specialOffer.forEach(function(value,item){
                var splitOffer = value.title.split("\n");

                splitOffer.forEach(function(value,item){
                    if(item < 5 && 1 < 2) {
                        offer.push(`<div class="item">`+ value +`</div>`);
                    }
                });
                
                if (splitOffer.length > 5) {
                    offer.push(`<div class="item">...</div>`);
                }
            })
        }

        return offer.join('');
    }
</script>


