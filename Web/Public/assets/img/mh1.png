                Hura.Template.render(holder, '<div class="owl-carousel owl-theme custom-nav">' + html + '</div>');
                run_carousel(holder);
                    
            } else{
                $('#js-same-category-container').remove();
            }
        })
    }  
  
  
    function _variantSelectedImage(img){
        const new_image = img.replace('75_','');
        $('#Zoomer img').attr('src', new_image);
  
  		$('#js-img-gallery .js-image-item img').each(function(){
